{
  "name": "pedeaii_api",
  "version": "1.0.0",
  "description": "An whatsapp bot for PedeAii",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm build && node dist/index.js",
    "dev": "tsx watch index.ts",
    "watch": "ts-node-dev --respawn --transpile-only index.ts",
    "knex": "npx knex --knexfile knexfile.ts",
    "migrate:make": "npm run knex migrate:make",
    "migrate:up": "npm run knex migrate:up",
    "migrate:down": "npm run knex migrate:down",
    "migrate:latest": "npm run knex migrate:latest",
    "migrate:list": "npm run knex migrate:list",
    "migrate:rollback": "npm run knex migrate:rollback",
    "seed:make": "npm run knex seed:make",
    "seed:run": "npm run knex seed:run",
    "build": "tsc build",
    "test": "jest"
  },
  "author": "WhatsApp Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.7",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "inversify": "^6.0.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "moment": "^2.30.1",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [],
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.14.10",
    "@types/pg": "^8.11.6",
    "@types/reflect-metadata": "^0.1.0",
    "@types/stack-trace": "^0.0.33",
    "express-async-errors": "^3.1.1",
    "jest": "^29.7.0",
    "stack-trace": "^1.0.0-pre2",
    "ts-jest": "^29.2.3",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3"
  }
}
