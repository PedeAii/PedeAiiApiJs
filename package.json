{
  "name": "pedeaii_api",
  "version": "1.0.0",
  "description": "An whatsapp bot for PedeAii",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm build && node dist/index.js",
    "dev": "tsx watch index.ts",
    "watch": "tsx watch index.ts",
    "knex": "npx knex --knexfile knexfile.ts",
    "migrate:make": "npm run knex migrate:make",
    "migrate:up": "npm run knex migrate:up",
    "migrate:down": "npm run knex migrate:down",
    "migrate:latest": "npm run knex migrate:latest",
    "migrate:list": "npm run knex migrate:list",
    "migrate:rollback": "npm run knex migrate:rollback",
    "seed:make": "npm run knex seed:make",
    "seed:run": "npm run knex seed:run",
    "build": "tsc build"
  },
  "author": "WhatsApp Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "moment": "^2.30.1",
    "pg": "^8.12.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [],
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.14.10",
    "@types/pg": "^8.11.6",
    "@types/stack-trace": "^0.0.33",
    "express-async-errors": "^3.1.1",
    "stack-trace": "^1.0.0-pre2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3"
  }
}
